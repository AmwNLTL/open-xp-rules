        $datafield14 = datafield14
        $datafield15 = datafield15
        $datafield16 = datafield16
        $datafield17 = datafield17
        $datafield18 = datafield18

        $alert.key = lower(join([subject.account.id, datafield1, object.process.cmdline], "|"))

        $reason = "elevation-success"
    }

emit {
    $correlation_type = "event"

    $subject = "account"
    $action = "execute"
    $object = "process"
    $status = "success"

    if $datafield5 == "true" then
        $importance = "low"
    elif $datafield5 == "false" and $reason == "elevation-attempt" then
        $importance = "medium" 
    elif $datafield5 == "false" and $reason == "elevation-success" then 
        $importance = "high"
    endif

    $category.generic = "Attack"
    $category.high = "Privilege Escalation"
    $category.low = "Abuse Elevation Control Mechanism: Elevated Execution with Prompt"

    $incident.aggregation.key = join([$correlation_name, lower($event_src.host), lower($subject.account.id)], "|")
    $incident.severity = $importance
    $incident.category = "SoftwareSuspiciousActivity"
    $incident.aggregation.timeout = 2h
}
