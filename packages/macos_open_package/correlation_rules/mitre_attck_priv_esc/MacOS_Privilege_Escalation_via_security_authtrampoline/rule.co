event Process_Start:
    key:
        event_src.host
    filter {
        filter::NotFromCorrelator()
        and event_src.title == "edr"
        and msgid == "9"
        and action == "execute"
        and lower(object.process.fullpath) == "/usr/libexec/security_authtrampoline"
        and (
            regex(lower(object.process.cmdline), "(\/usr\/libexec\/)?security_authtrampoline\s+.*\s+(auth\s+\d+)?\s+.*", 0) != null
            )
    }

event Process_Start_with_Elevated_Privileges: 
    key:
        event_src.host
    filter {
        filter::NotFromCorrelator()
        and event_src.title == "edr"
        and msgid == "9"
        and action == "execute"
        and lower(subject.process.fullpath) == "/usr/libexec/security_authtrampoline"
        and datafield1 == "0"
    }

rule MacOS_Privilege_Escalation_via_security_authtrampoline: Process_Start -> Process_Start_with_Elevated_Privileges? timer 30s

    on Process_Start {
        $action = action
        $object = object
        $status = status

        $subject.account.name = subject.account.name
